# Example environment variables for Postman Without bugs

# Database
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=postman_db

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT
JWT_SECRET=your_jwt_secret

# App
PORT=8080
ENV=development

# Other
LOG_LEVEL=info
REQUEST_TIMEOUT=10s
MAX_REDIRECTS=5
MAX_RESPONSE_SIZE=1048576
ALLOW_LOCALHOST=false
ALLOW_PRIVATE_IPS=false
